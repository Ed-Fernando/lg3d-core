                <node name="win-assoc"><!-- user:/org/jdesktop/lg3d/displayserver/nativewindow/x11/win-assoc -->
                  <!-- Default user configuration for window-on-the-backside feature -->
                  <map/>
                  <!-- 
                    List of a set of app info (6 entries per app).
                    - parent-res-cls       - parent app's resource class in X
                    - parent-res-name      - parent app's resource name in X
                    - parent-title-pattern - regular expression pattern to match with the window title
                  -->
                  
                  <node name="Firefox-bin">
                    <map>
                      <entry key="parent-res-cls"       value="Firefox-bin"/>
                      <entry key="parent-res-name"      value="Gecko"/>
                      <entry key="parent-title-pattern" value="^.* Firefox$"/>   
                      <entry key="child-res-cls"        value="Firefox-bin"/>
                      <entry key="child-res-name"       value="Gecko"/>
                      <entry key="child-title-pattern"  value="^(Firefox |)Preferences$"/>
                    </map>
                  </node>
                  
                  <node name="Gnome-terminal">
                    <map>
                      <entry key="parent-res-cls"       value="Gnome-terminal"/>
                      <entry key="parent-res-name"      value="gnome-terminal"/>
                      <entry key="parent-title-pattern" value="^.*$"/> 
                      <entry key="child-res-cls"        value="Gnome-terminal"/>
                      <entry key="child-res-name"       value="gnome-terminal"/>
                      <entry key="child-title-pattern"  value="^Profiles$|^Keyboard Shortcuts$|^Editing [Pp]rofile .*$"/>
                    </map>
                  </node>
                </node>
                
              </node><!-- end of node name="x11"-->
